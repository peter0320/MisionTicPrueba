@page
@model ProyectoCiclo3.App.Frontend.Pages.DetailsServicioModel
@{
    var servicio = Model.Services;
}
 
<ul class="list-group list-group-flush">
  <li class="list-group-item"> <b>id:</b> @servicio.id </li>
  <li class="list-group-item"> <b>Origen:</b> @servicio.origen.nombre @servicio.origen.apellidos @servicio.origen.ciudad</li>
  <li class="list-group-item"> <b>Destino:</b> @servicio.destino.nombre @servicio.destino.apellidos @servicio.destino.ciudad </li>
  <li class="list-group-item"> <b>Fecha de Envio:</b> @servicio.fecha </li>
  <li class="list-group-item"> <b>Hora de Envio:</b> @servicio.hora </li>  
  <li class="list-group-item"> <b>Encomienda:</b> @servicio.encomienda.descripcion </li>  
</ul>	

<a class="btn btn-info" asp-page="./Edit" asp-route-servicioId="@servicio.id">Editar</a>
  
<a class="btn btn-primary" asp-page="/Servicio/List" >Volver</a>